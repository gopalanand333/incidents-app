{
  "name": "@capire/incidents",
  "version": "1.0.0",
  "calesi": {
    "@cap-js/audit-logging": "*",
    "@cap-js/change-tracking": "*",
    "@cap-js/notifications": "*",
    "@cap-js/attachments": "*",
    "@cap-js/telemetry": "*",
    "@sap/ams": "*",
    "@sap/ams-dev": "*",
    "dummy": 0
  },
  "dependencies": {
    "@cap-js/telemetry": ">=0",
    "@grpc/grpc-js": "^1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.57.2",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.57.2",
    "@opentelemetry/host-metrics": "0.35.5",
    "@sap/cds": ">=8",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/attachments": "^1",
    "@cap-js/audit-logging": "^0.8.0",
    "@cap-js/cds-test": "^0",
    "@cap-js/change-tracking": "^1.0.6",
    "@cap-js/sqlite": "^1.0.1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "^1.108.4"
  },
  "scripts": {
    "watch": "cds watch",
    "start": "cds-serve",
    "test": "npx jest --silent"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/xmpls/"
    ]
  },
  "sapux": [
    "app/incidents"
  ],
  "cds": {
    "requires": {
      "auth": {
        "[development]": {
          "users": {
            "alice": {
              "roles": [
                "support",
                "admin"
              ]
            },
            "bob": {
              "roles": [
                "support"
              ]
            }
          }
        }
      },
      "[production]": {
        "telemetry": {
          "kind": "to-cloud-logging"
        }
      },
      "destinations": true,
      "html5-runtime": true,
      "workzone": true
    }
  },
  "private": true
}
